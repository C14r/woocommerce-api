{
    "name": "c14r/woocommerce-api",
    "type": "library",
    "description": "A Wrapper for the WooCommerce REST-API",
    "keywords": [
        "http",
        "rest",
        "client",
        "HTTP client",
        "woocommerce",
        "wrapper"
    ],
    "homepage": "http://www.c14r.de/",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Christian Pfeifer",
            "email": "mail@christian-pfeifer.de",
            "homepage": "http://www.christian-pfeifer.de/"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*",
        "automattic/woocommerce": "^3.1",
        "illuminate/support": "^9.0|^10.23.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.12"
    },
    "autoload": {
        "psr-4": {
            "C14r\\Woocommerce\\": "src/Woocommerce/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "C14r\\Woocommerce\\Providers\\WoocommerceServiceProvider"
            ]
        }
    }
}
